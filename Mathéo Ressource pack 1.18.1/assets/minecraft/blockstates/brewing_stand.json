{
    "multipart": [
        {   "apply": { "model": "block/brewing_stand/brewing_stand_empty" }},
        {   "when": { "has_bottle_0": "true" },
            "apply": { "model": "block/brewing_stand/brewing_stand_bottles_1" }
        },
        {   "when": { "has_bottle_1": "true" },
            "apply": { "model": "block/brewing_stand/brewing_stand_bottles_2" }
        },
        {   "when": { "has_bottle_2": "true" },
            "apply": { "model": "block/brewing_stand/brewing_stand_bottles_3" }
        },
        {   "when": { "has_bottle_0": "false" },
            "apply": { "model": "block/brewing_stand/brewing_stand_bottles_1_empty" }
        },
        {   "when": { "has_bottle_1": "false" },
            "apply": { "model": "block/brewing_stand/brewing_stand_bottles_2_empty" }
        },
        {   "when": { "has_bottle_2": "false" },
            "apply": { "model": "block/brewing_stand/brewing_stand_bottles_3_empty" }
        }
    ]
}
